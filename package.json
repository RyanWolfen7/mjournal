{
  "name": "mjournal",
  "description": "Minimalist journaling web application",
  "version": "0.1.0",
  "homepage": "http://mjournal.peterlyons.com",
  "repository": "ssh://git.peterlyons.com/home/plyons/projects/mjournal.git",
  "author": "Peter Lyons <pete@peterlyons.com> (http://peterlyons.com)",
  "engines": {
    "node": "~0.10.28"
  },
  "scripts": {
    "start": "./bin/go express",
    "test": "./bin/go test"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "brfs"
    ]
  },
  "browser": {
    "angular": "./bower_components/angular/angular.js",
    "angular-resource": "./bower_components/angular-resource/angular-resource.js",
    "ng-tags-input": "./bower_components/ng-tags-input/ng-tags-input.js"
  },
  "browserify-shim": {
    "angular": {
      "exports": "angular"
    },
    "angular-resource": {
      "depends": [
        "./bower_components/angular/angular.js:angular"
      ]
    },
    "ng-tags-input": {
      "depends": [
        "./bower_components/angular/angular.js:angular"
      ]
    }
  },
  "dependencies": {
    "any-db": "^2.1.0",
    "any-db-postgres": "^2.1.3",
    "bcrypt": "^0.7.8",
    "body-parser": "^1.4.3",
    "brfs": "^1.1.2",
    "browserify-middleware": "^3.0.0",
    "browserify-shim": "^3.6.0",
    "bunyan": "^0.23.1",
    "commander": "^2.2.0",
    "concat-stream": "^1.4.6",
    "config3": "0.0.0",
    "connect-pg-simple": "^2.0.0",
    "cookie-parser": "^1.3.2",
    "express": "^4.6.1",
    "express-session": "^1.6.5",
    "gesundheit": "^0.10.0",
    "jade": "^1.3.1",
    "lodash": "^2.4.1",
    "pg": "^3.4.0",
    "promptly": "~0.2.0",
    "stylus": "^0.47.1"
  },
  "devDependencies": {
    "bower": "^1.3.8",
    "cheerio": "^0.17.0",
    "eslint": "^0.7.4",
    "expectacle": "~1.3.0",
    "mocha": "^1.20.1",
    "node-dev": "^2.3.0",
    "node-inspector": "^0.7.4",
    "npm-check-updates": "~1.2.0",
    "sinon": "^1.10.3",
    "supertest": "^0.13.0",
    "watchify": "^0.10.2"
  }
}
