{
  "name": "mjournal",
  "description": "Minimalist journaling web application",
  "version": "1.0.1",
  "homepage": "http://mjournal.peterlyons.com",
  "repository": "ssh://git.peterlyons.com/home/plyons/projects/mjournal.git",
  "author": "Peter Lyons <pete@peterlyons.com> (http://peterlyons.com)",
  "engines": {
    "node": "0.10.31"
  },
  "scripts": {
    "start": "./bin/go express",
    "test": "./bin/go test",
    "postinstall": "./node_modules/.bin/bower --config.analytics=false install"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "brfs"
    ]
  },
  "browser": {
    "angular": "./bower_components/angular/angular.js",
    "angular-resource": "./bower_components/angular-resource/angular-resource.js",
    "ng-tags-input": "./bower_components/ng-tags-input/ng-tags-input.js"
  },
  "browserify-shim": {
    "angular": {
      "exports": "angular"
    },
    "angular-resource": {
      "depends": [
        "./bower_components/angular/angular.js:angular"
      ]
    },
    "ng-tags-input": {
      "depends": [
        "./bower_components/angular/angular.js:angular"
      ]
    }
  },
  "dependencies": {
    "async": "^0.9.0",
    "bcryptjs": "^2.0.2",
    "body-parser": "^1.4.3",
    "bole": "^0.1.0",
    "bower": "^1.3.8",
    "brfs": "^1.2.0",
    "browserify-middleware": "^3.0.1",
    "browserify-shim": "^3.6.0",
    "commander": "^2.2.0",
    "config3": "0.0.0",
    "connect-pg-simple": "^2.1.1",
    "cookie-parser": "^1.3.2",
    "dev-null": "^0.1.1",
    "express": "^4.6.1",
    "express-session": "^1.6.5",
    "httperrors": "^0.5.0",
    "jade": "^1.3.1",
    "joi": "^4.6.2",
    "knex": "^0.6.22",
    "lodash": "^2.4.1",
    "mustache": "^0.8.2",
    "pg.js": "^3.4.2",
    "promptly": "~0.2.0",
    "rand-token": "^0.2.1",
    "split": "^0.3.0",
    "stylus": "^0.47.1"
  },
  "devDependencies": {
    "bistre": "^1.0.0",
    "cheerio": "^0.17.0",
    "eslint": "^0.7.4",
    "expectacle": "~1.3.0",
    "mocha": "^1.20.1",
    "mocha-sinon": "^1.1.3",
    "node-dev": "^2.3.0",
    "node-inspector": "^0.7.4",
    "npm-check-updates": "~1.2.0",
    "sinon": "^1.10.3",
    "supertest": "^0.13.0",
    "supertest-session": "0.0.3"
  }
}
